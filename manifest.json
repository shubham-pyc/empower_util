{
  "manifest_version": 2,
  "name": "Enhanced Empower",
  "version": "1.0.0",
  "content_scripts": [{
    "matches": ["https://empower.impetus.co.in/*"],
    "js": [
      "jquery-3.3.1.min.js",
      "js/objects/exception-handler.js",
      "js/lib/create.js",
